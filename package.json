{
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "dev:live": "tsx --env-file=.env src/index.ts",
    "dev": "tsx --env-file=.env.dev src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --minify",
    "serve": "http-server ./",
    "seed": "prisma db seed",
    "migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "6.3.0",
    "axios": "^1.7.9",
    "big-integer": "^1.6.52",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "node-html-to-image": "^5.0.0",
    "pino": "^9.6.0",
    "pino-abstract-transport": "^2.0.0",
    "pino-pretty": "^13.0.0",
    "sonic-boom": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "prisma": "^6.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "puppeteer"
    ]
  }
}